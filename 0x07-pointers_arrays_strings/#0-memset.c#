#include "main.h"
/**
 *program for function that fills memory
 *with a constant byte
 *
 *return memory on (success)
 */
void *_memset(void *s, int c, size_t n)

{
	unsigned int index;
	unsigned char *memory = s, value = c;

        for (index = 0; index < n; index++)

		memory[index] = value;

	return (memory);
}
